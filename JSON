

{
  "file_path": "students.csv"
}


{
  "id": 1,
  "name": "John",
  "min_total_marks": 80
}

curl -X GET "http://localhost:5000/api/students?page=1&page_size=10"

curl -X POST -H "Content-Type: application/json" -d '{
  "id": 1,
  "name": "John",
  "min_total_marks": 80
}' "http://localhost:5000/api/students/filter"
